@model IEnumerable<OdeToFood.Models.Restaurant>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>OdeToFood</title>
</head>
<body>
    <h1>Welcome!</h1>
    <table>
        @foreach (var restaraunt in Model)
        {
            <tr>
                <td>@restaraunt.Id</td>
                <td>@restaraunt.Name</td>
            </tr>
        }
    </table>
</body>
</html>